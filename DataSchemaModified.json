{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HumidityArtSocialDB",
  "type": "object",
  "properties": {
    "user": { 
      "type": "object",
      "properties": {
        "user_id": { "type": "string" },
        "name": "string",
        "email": { "type": "string", "format": "email" }
      }
    },
    "activity": {
      "type": "object",
      "properties": {
        "date": { "type": "string", "format": "date" },
        "humidity_level": { "type": "number" },
        "art_interest": { "type": "string" }
      }
    }
  }
}
